    <property name="MSalaryTypeID" column="MSalaryTypeID" />
    <property name="MEmpDsgID" column="MEmpDsgID" />
    <property name="Salary" column="Salary" />
    <property name="GivenTo" column="GivenTo" />
    <property name="Description" column="Description" />
    <property name="AmountSpent" column="AmountSpent" />
    <property name="ExtraCharges" column="ExtraCharges" />
    <property name="PaymentDate" column="PaymentDate" />    
    <property name="ObsInd" column="ObsInd" />
    <property name="LastModifiedBy" column="LastModifiedBy" />
    <property name="LastModifiedDate" column="LastModifiedDate" />

  </class>
</hibernate-mapping>

