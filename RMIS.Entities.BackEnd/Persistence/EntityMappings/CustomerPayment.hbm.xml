<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="RMIS.Entities.BackEnd"
                   namespace="RMIS.Entities.BackEnd">

  <class name="CustomerPayment" table="[dbo].[CustomerPayment]" optimistic-lock="none" >
    <id name="CustPaymentID" column="CustPaymentID" >
      <generator class="assigned"/>
    </id>
    <property name="CustID" column="CustID" />
    <property name="OutstandingAmount" column="OutstandingAmount" />
    <property name="Status" column="Status" />
    <property name="ObsInd" column="ObsInd" />
    <property name="LastModifiedBy" column="LastModifiedBy" />
    <property name="LastModifiedDate" column="LastModifiedDate" />

  </class>
</hibernate-mapping>

