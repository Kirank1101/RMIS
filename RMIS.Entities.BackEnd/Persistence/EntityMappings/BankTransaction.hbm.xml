<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="RMIS.Entities.BackEnd"
                   namespace="RMIS.Entities.BackEnd">

  <class name="BankTransaction" table="[dbo].[BankTransaction]" optimistic-lock="none" >
    <id name="BankTransID" column="BankTransID" >
      <generator class="assigned"/>
    </id>   
    <property name="CustID" column="CustID" />
    <property name="Description" column="Description" />
    <property name="Withdraw" column="Withdraw" />
    <property name="Deposit" column="Deposit" />
    <property name="Balance" column="Balance" />
    <property name="TransactionDate" column="TransactionDate" />
    <property name="ObsInd" column="ObsInd" />
    <property name="LastModifiedBy" column="LastModifiedBy" />
    <property name="LastModifiedDate" column="LastModifiedDate" />

  </class>
</hibernate-mapping>

